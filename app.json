{
    "expo": {
        "name": "TimeCardTracker-Expo",
        "slug": "TimeCardTracker-Expo",
        "version": "0.0.1",

        "scheme": "TimeCardTracker-Expo",
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["expo-router"],
        "experiments": {
            "typedRoutes": true,

            "tsconfigPaths": true
        },

        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#000000"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png"
            }
        }
    }
}
